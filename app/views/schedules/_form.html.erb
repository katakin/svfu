<%= simple_form_for(@schedule) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :parity %>
    <%= f.input :occupation %>
    <%= f.input :teacher_id %>
    <%= f.input :pair_id %>
    <%= f.input :room_id %>
    <%= f.input :subject_id %>
    <%= f.input :weekday_id %>
    <%= f.input :group_id %>
    <%= f.input :deleted %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

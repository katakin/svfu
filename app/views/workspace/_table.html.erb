<table>
  <tr>
    <th>Parity</th>
    <th>Occupation</th>
    <th>Teacher</th>
    <th>Pair</th>
    <th>Room</th>
    <th>Subject</th>
    <th>Weekday</th>
    <th>Group</th>
    <th>Deleted</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @schedules.each do |schedule| %>
  <tr>
    <td><%= schedule.parity %></td>
    <td><%= schedule.occupation %></td>
    <td><%= schedule.teacher.fio %></td>
    <td><%= schedule.pair.name %></td>
    <td><%= schedule.room.name %></td>
    <td><%= schedule.subject.name %></td>
    <td><%= schedule.weekday.name %></td>
    <td><%= schedule.group.name %></td>
    <td><%= schedule.deleted %></td>
    <td><%= link_to 'Show', schedule %></td>
    <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
    <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>